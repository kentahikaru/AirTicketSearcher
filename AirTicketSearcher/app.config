<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog" />
  </configSections>

<appSettings>
    <add key="test1" value="test1val"/>
    <add key="test2" value="test2val=="/>
    
  </appSettings>
